<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tp.travelplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainTrips" />
        <activity android:name=".Register" />
        <activity android:name=".AddNewTrip" />
        <activity android:name=".EachTripInfo" />
        <activity android:name=".TripUpdateDelete" />
        <activity android:name=".Flights" />
        <activity android:name=".AddNewFlight" />
        <activity android:name=".FlightUpdateDelete" />
        <activity android:name=".AddNewAccommodation" />
        <activity android:name=".Accommodation" />
        <activity android:name=".AccUpdateDelete"></activity>
    </application>

</manifest>